{
  "manifest_version": 2,
  "name": "Super Playback",
  "description": "Control More Time",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action":{
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
//      "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js","content.js"]
    }
  ]

}
